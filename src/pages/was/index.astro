---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { fetchPosts } from '../../lib/strapi';
const pageTitle = 'All Blog Posts';

const posts = await fetchPosts('was-posts');
---

<BaseLayout title={pageTitle}>
  <p>This is where I will post about WAS</p>
  <ul>
    {
      posts.map((post) => (
        <li>
          <a href={`/was/${post.slug}`}>{post.title}</a>
        </li>
      ))
    }
  </ul>
</BaseLayout>
